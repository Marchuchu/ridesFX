<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>


<AnchorPane fx:id="mainWrapper" prefHeight="400.0" prefWidth="773.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eus.ehu.ridesfx.uicontrollers.MessageController">
   <children>
      <TextArea fx:id="messageTXT" layoutX="395.0" layoutY="53.0" prefHeight="267.0" prefWidth="325.0" />
      <TableView fx:id="messageTable" layoutX="64.0" layoutY="53.0" prefHeight="243.0" prefWidth="263.0">
        <columns>
          <TableColumn prefWidth="75.0" text="%From" />
          <TableColumn prefWidth="83.0" text="%Subject" />
            <TableColumn prefWidth="67.0" text="%Message" />
        </columns>
      </TableView>
      <Button fx:id="seeMessageBttn" layoutX="153.0" layoutY="343.0" mnemonicParsing="false" onAction="#onClickSeeMessage" text="%SeeMessage" />
      <Button fx:id="sendNewMessageBttn" layoutX="499.0" layoutY="343.0" mnemonicParsing="false" onAction="#onClickSendNewMessage" text="%SendNewMessage" />
      <Text fx:id="errorM" layoutX="147.0" layoutY="325.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      <Text fx:id="errorMessage" layoutX="174.0" layoutY="324.0" strokeType="OUTSIDE" strokeWidth="0.0" text="%empty" />
   </children>
</AnchorPane>
